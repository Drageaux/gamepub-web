{
  "name": "gamepub-web",
  "version": "0.0.0",
  "scripts": {
    "ng": "ng",
    "dev": "ng serve",
    "build": "ng build",
    "watch": "ng build --watch --configuration development",
    "test": "ng test",
    "heroku-postbuild": "cross-env-shell ng build --configuration=$ENV && npm install -g http-server-spa",
    "start": "http-server-spa dist/gamepub-web index.html $PORT",
